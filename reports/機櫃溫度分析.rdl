    <DataSet Name="機櫃溫度分析">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@廠區">
            <Value>=Parameters!廠區.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@樓棟">
            <Value>=Parameters!樓棟.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@機房">
            <Value>=Parameters!機房.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@月份">
            <Value>=Parameters!月份.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@機櫃">
            <Value>=Parameters!機櫃.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@年份">
            <Value>=Parameters!年份.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

select Group機櫃名稱.日期 as 日期, avg(Group機櫃名稱.值) as 值,MAX(Group機櫃名稱.值) as 最高值, SUBSTRING(Group機櫃名稱.機櫃名稱,1,4) as 機櫃名稱, SUBSTRING(Group機櫃名稱.機櫃名稱, 5,2) 風向 from
(
select CONVERT(nchar(10),CreateDate,120) as 日期, CONVERT(float,Value) as 值, SUBSTRING(B.SensorName,1,10)as 機櫃名稱
from MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
where A.DataID = C.ID
and C.SensorID = B.ID
and C.ControlID = D.ID
and D.Factory = @廠區---B.SensorName like '%電力'
and D.Building = @樓棟
and D.ComputerRoom = @機房 
and ISNUMERIC(A.Value)&gt;0
and DATEPART(MM,CreateDate) = @月份
and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51 
and C.Type = '溫度'
and B.SensorName like '%機櫃%'
and (B.SensorName like +'%'+ @機櫃+'%'or @機櫃 = ' ')
) as Group機櫃名稱
group by Group機櫃名稱.日期,Group機櫃名稱.機櫃名稱</CommandText>
      </Query>
      <Fields>
        <Field Name="日期">
          <DataField>日期</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="值">
          <DataField>值</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="機櫃名稱">
          <DataField>機櫃名稱</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="最高值">
          <DataField>最高值</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="風向">
          <DataField>風向</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
