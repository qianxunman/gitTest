    <DataSet Name="ListSensorName">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>select CONVERT(char(6),SensorName) as SensorName  ,3 as od
 from SensorInfo
where SensorName like '%機櫃%風'
union
select ' ',2 
union 
select 'F2機櫃',1 
order by od</CommandText>
      </Query>
      <Fields>
        <Field Name="SensorName">
          <DataField>SensorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="od">
          <DataField>od</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
