            <Value>=Parameters!年份.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
--author:luojun
--Modificator:luojun
--2017-8-2 16:34:04
--Modification description:To select the data which not include the Celeriry Data

declare @EndTime datetime,@StartYear datetime,@EndYear datetime
if(DATEPART(MM,GETDATE()) = @月份)
set @EndTime = CONVERT(char(10),GETDATE(),120)--若是本月,則終止時間為今天0點
else 
set @EndTime = DATEADD(MONTH,1,CONVERT (datetime,CONVERT(char(5),GETDATE(),120) + @月份 + '-01'))--非本月則終止時間為下月1號0點

set @StartYear = CONVERT (char(4),@年份)+'-01-01'
set @EndYear = DATEADD(YEAR,1,CONVERT (char(4),@年份)+'-01-01')

select MainData.Type
,MainData.RealCount
,MainData.ShouldCount* TypeCount.TypeCount as ShouldCount1
,MainData.ShouldCount* TypeCount.TypeCount-MainData.RealCount as LoseCountReal
,case when MainData.ShouldCount* TypeCount.TypeCount-MainData.RealCount&lt;0 then 0
else MainData.ShouldCount* TypeCount.TypeCount-MainData.RealCount
end as LoseCount1

 from 
(
		select  
		C.Type,COUNT(*) as RealCount
		
		
		--A.CreateDate,A.Value, C.Type,B.SensorName 
		,D.NormaFreq
		,DATEDIFF(SECOND,CONVERT (datetime,CONVERT(char(5),GETDATE(),120) + @月份 + '-01'),@EndTime) as diffTime
		,DATEDIFF(SECOND,CONVERT (datetime,CONVERT(char(5),GETDATE(),120) + @月份 + '-01'),@EndTime)/D.NormaFreq as ShouldCount
		,DATEDIFF(SECOND,CONVERT (datetime,CONVERT(char(5),GETDATE(),120) + @月份 + '-01'),@EndTime)/D.NormaFreq-COUNT(*) as LoseCount
		
		from

			MonitData as A ,
			SensorInfo as B,
			AddressInfo as C,
			ControlInfo as D
			where 
			A.DataID = C.ID
			and C.SensorID = B.ID
			and C.ControlID = D.ID
			and D.Factory = @廠區---B.SensorName like '%電力'
			and D.Building = @樓棟
			and D.ComputerRoom = @機房
			and 
			--DATEPART(YYYY,CreateDate)
			--CreateDate &gt; CONVERT (char(4),@年份)+'-01-01'--年份限制,默認選擇今年:2017-7-17 16:08:05,取消:2017-8-7 15:07:17
			CreateDate&gt;@StartYear
			and CreateDate&lt;@EndYear--2017-8-7 15:06:45
			and DATEPART(MM,CreateDate) = @月份
			--and CreateDate&gt;'2017-06-01'
			and CreateDate&lt;@EndTime
			and A.Celerity = 'N'
		group by C.Type,D.NormaFreq
			
) as MainData
left join 
(
			select  COUNT(*)as TypeCount,Type from AddressInfo as C,ControlInfo as D
			where C.ControlID = D.ID
			and D.Factory = @廠區
			and D.Building = @樓棟
			and D.ComputerRoom = @機房

			group by Type

)as TypeCount
on MainData.Type = TypeCount.Type</CommandText>
      </Query>
      <Fields>
        <Field Name="Type">
          <DataField>Type</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="RealCount">
          <DataField>RealCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ShouldCount1">
          <DataField>ShouldCount1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LoseCount1">
          <DataField>LoseCount1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LoseCountReal">
          <DataField>LoseCountReal</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
