--說明：
--統計每月每個數據採集量與丟包數據量的比例


--開發時間:2017-6-16 16:40:16
--修改時間:2017-6-19 11:31:06
--開發人:羅俊

declare @EndTime datetime,@StartTime Datetime
set @EndTime = GETDATE()
set @StartTime = CONVERT(char(10),GETDATE(),120)
select 
MainData1.RealCount as RealCount
,SensorCount.SensorCount as ShouldCount1
,SensorCount.SensorCount-MainData1.RealCount as LoseCount1
 from 
(

	select COUNT(*) RealCount
	from
	(
				select  
				B.ID,COUNT(*) CountBySensorID
				--C.Type,COUNT(*) as RealCount
				
				
				----A.CreateDate,A.Value, C.Type,B.SensorName 
				--,D.NormaFreq
				--,DATEDIFF(SECOND,@StartTime,@EndTime) as diffTime
				--,DATEDIFF(SECOND,@StartTime,@EndTime)/D.NormaFreq as ShouldCount
				--,DATEDIFF(SECOND,@StartTime,@EndTime)/D.NormaFreq-COUNT(*) as LoseCount
				
				from

					MonitData as A ,
					SensorInfo as B,
					AddressInfo as C,
					ControlInfo as D
					where 
					A.DataID = C.ID
					and C.SensorID = B.ID
					and C.ControlID = D.ID
					and D.Factory = @廠區
					and D.Building = @樓棟
					and D.ComputerRoom = @機房
					and 
					CreateDate &gt; @StartTime
					and CreateDate&lt;@EndTime
				group by B.ID

	) as MainData

)as MainData1
left join
(
	select  COUNT(*)as SensorCount from SensorInfo as B, ControlInfo as D
	where B.ControlID = D.ID
	and D.Factory = @廠區
	and D.Building = @樓棟
	and D.ComputerRoom = @機房

) as SensorCount
on 1=1
</CommandText>
      </Query>
      <Fields>
        <Field Name="RealCount">
          <DataField>RealCount</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="ShouldCount1">
          <DataField>ShouldCount1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="LoseCount1">
          <DataField>LoseCount1</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
