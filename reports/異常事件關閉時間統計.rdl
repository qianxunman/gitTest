---報表:
--異常事件關閉時間統計
--說明：
--統計每月已關閉的異常事件和未關閉的異常事件

--開發時間:2017-6-16 11:19:26
--開發人:羅俊

select CreateDate,Times,COUNT(*) as Count from

(

select CONVERT(char(10),CreateDate,120) CreateDate,CloseDate
--,DATEDIFF(HH,CreateDate,A.CloseDate)
,Case when DATEDIFF(HH,CreateDate,A.CloseDate)&lt;1 and CloseDate!='1900-01-01 00:00:00.000' then '1小時內'
when DATEDIFF(HH,CreateDate,A.CloseDate)&gt;=1 and DATEDIFF(HH,CreateDate,A.CloseDate)&lt;6 and CloseDate!='1900-01-01 00:00:00.000' then '6小時內'
when DATEDIFF(HH,CreateDate,A.CloseDate)&gt;=6 and DATEDIFF(HH,CreateDate,A.CloseDate)&lt;24 and CloseDate!='1900-01-01 00:00:00.000' then '1天內'
when DATEDIFF(HH,CreateDate,A.CloseDate)&gt;=24 and DATEDIFF(HH,CreateDate,A.CloseDate)&lt;72 and CloseDate!='1900-01-01 00:00:00.000' then '3天內'
when DATEDIFF(HH,CreateDate,A.CloseDate)&gt;=72 and DATEDIFF(HH,CreateDate,A.CloseDate)&lt;168 and CloseDate!='1900-01-01 00:00:00.000' then '7天內'
when DATEDIFF(HH,CreateDate,A.CloseDate)&gt;=168 and CloseDate!='1900-01-01 00:00:00.000' then '大於7天'
else '至今未關閉'
end as Times

 from ExceptionDeal as A,SensorInfo as B,AddressInfo as C, ControlInfo as D
where  A.DataID = C.ID
and C.ControlID = D.ID 
and C.SensorID = B.ID
and D.Factory = @廠區---B.SensorName like '%電力'
and D.Building = @樓棟
and D.ComputerRoom = @機房
and (DATEPART(MM,CreateDate) = @月份 or @月份 = '0')
and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-18 08:11:56

) as TB
group by CreateDate,Times
order by CreateDate</CommandText>
      </Query>
      <Fields>
        <Field Name="CreateDate">
          <DataField>CreateDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Times">
          <DataField>Times</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Count">
          <DataField>Count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
