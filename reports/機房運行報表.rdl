						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
						and B.SensorName like '%相'
						and (C.Type = '有功功率' )
						and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
				)Result
				 join
				(-----------------------------這是作為一個查詢條件

					select  GP_SensorName.SensorName,MIN(DiffTime) MinDiffTime , GP_SensorName.CreatDate 
					from
					(
						select A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
						,case
						 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 end as DiffTime


						 from 
							MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
							where A.DataID = C.ID
							and C.SensorID = B.ID
							and C.ControlID = D.ID
							and D.Factory = @廠區
							and D.Building = @樓棟
							and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
							and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
							and B.SensorName like '%相'
							and (C.Type = '有功功率' )
							and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
					) GP_SensorName
					group by GP_SensorName.SensorName,GP_SensorName.CreatDate
				)Condition
				on Result.CreatDate = Condition.CreatDate
				and Result.DiffTime = Condition.MinDiffTime
				and Result.SensorName = Condition.SensorName
			) as NearValue
		) 
		as Current1
		order by 日期
	)GP_Current
	group by GP_Current.日期, GP_Current.SensorName

) as PNow
on Voltage.日期 = PNow.日期
and Voltage.SensorName = PNow.SensorName
left join

(

select top 100 percent GP_Current.日期,GP_Current.SensorName, MAX(GP_Current.A)as A, MAX(GP_Current.B) as B ,MAX(GP_Current.C) as C from
(

	select top 100 percent Current1.日期, Current1.值, Current1.三相電流類別,Current1.SensorName,
	case when Current1.三相電流類別 = 'A' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'A')
	end as A,
	case when Current1.三相電流類別 = 'B' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'B')
	end as B,
	case when Current1.三相電流類別 = 'C' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'C')
	end as C
	 from
		(
		select top 100 percent   CONVERT(nchar(16),NearValue.CreatDate,120) as 日期, CONVERT(float,Value) as 值, NearValue.Type, SUBSTRING(NearValue.SensorName,1,4) SensorName,
		case 
		when PATINDEX('%C%', NearValue.SensorName) &gt;0  then 'C'
		when PATINDEX('%B%', NearValue.SensorName) &gt;0  then 'B'
		when PATINDEX('%A%', NearValue.SensorName) &gt;0  then 'A'
		end as 三相電流類別
		from 
		(
		
----------------------------------------------------------------------------分割線，以下為求出的最接近時間點的數據

			select Result.* from
			(
				select A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
				,case
				 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
				 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
				 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
				 end as DiffTime


				 from 
					MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
					where A.DataID = C.ID
					and C.SensorID = B.ID
					and C.ControlID = D.ID
					and D.Factory = @廠區
					and D.Building = @樓棟
					and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
					and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
					and B.SensorName like '%相'
					and (C.Type = '功率因數' )
					and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
			)Result
			 join
			(-----------------------------這是作為一個查詢條件

				select  GP_SensorName.SensorName,MIN(DiffTime) MinDiffTime , GP_SensorName.CreatDate 
				from
				(
					select A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
					,case
					 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 end as DiffTime


					 from 
						MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
						where A.DataID = C.ID
						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
						and B.SensorName like '%相'
						and (C.Type = '功率因數' )
						and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
						
				) GP_SensorName
				group by GP_SensorName.SensorName,GP_SensorName.CreatDate
			)Condition
			on Result.CreatDate = Condition.CreatDate
			and Result.DiffTime = Condition.MinDiffTime
			and Result.SensorName = Condition.SensorName
----------------------------------------------------------------------------分割線，以上為求出的最接近時間點的數據	
		) as NearValue
	) 
	as Current1
	order by 日期
)GP_Current
group by GP_Current.日期, GP_Current.SensorName
) as Factor
on Voltage.日期 = Factor.日期
and Voltage.SensorName = Factor.SensorName</CommandText>
