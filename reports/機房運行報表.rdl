----------------------------------------------------結果匯總
select
 Voltage.日期, Voltage.SensorName, Voltage.AB, Voltage.AC, Voltage.BC,  Current1.A, Current1.B, Current1.C, PMax.A as 'PMaxA', PMax.B as 'PMaxB' ,PMax.C as 'PMaxC',PNow.A as 'PNowA', PNow.B as 'PNowB', PNow.C as 'PNowC'
 ,Factor.A as 'FA' , Factor.B as 'FB', Factor.C as 'FC'
 from
(

	select Voltage1.日期, MAX(Voltage1.值)MAXValue,Voltage1.SensorName, MAX(Voltage1.AB) AB  ,MAX(Voltage1.BC) BC, MAX(Voltage1.AC) AC

	 from 
	(
		select Voltage.日期, Voltage.值, Voltage.交流電壓類別,Voltage.SensorName,
		case when Voltage.交流電壓類別 = 'AB' then (select Voltage.值 where Voltage.交流電壓類別 = 'AB')
		end as AB,
		case when Voltage.交流電壓類別 = 'BC' then (select Voltage.值 where Voltage.交流電壓類別 = 'BC')
		end as BC,
		case when Voltage.交流電壓類別 = 'AC' then (select Voltage.值 where Voltage.交流電壓類別 = 'AC')
		end as AC
		 from
			(
			select  top 100 percent CONVERT(nchar(16),MinDiffTimeValue.CreatDate,120) as 日期, CONVERT(float,Value) as 值, MinDiffTimeValue.Type, SUBSTRING(MinDiffTimeValue.SensorName,1,4) SensorName,
			case 
			when (PATINDEX('%B%', MinDiffTimeValue.SensorName) &gt;0 and PATINDEX('%A%', MinDiffTimeValue.Type)&gt; 0) or (PATINDEX('%A%', MinDiffTimeValue.SensorName)&gt; 0 and PATINDEX('%B%', MinDiffTimeValue.Type)&gt; 0) then 'AB'
			when (PATINDEX('%B%', MinDiffTimeValue.SensorName) &gt;0 and PATINDEX('%C%', MinDiffTimeValue.Type)&gt; 0) or (PATINDEX('%C%', MinDiffTimeValue.SensorName) &gt;0 and PATINDEX('%B%', MinDiffTimeValue.Type)&gt; 0) then 'BC'
			when (PATINDEX('%A%', MinDiffTimeValue.SensorName) &gt;0 and PATINDEX('%C%', MinDiffTimeValue.Type)&gt; 0) or (PATINDEX('%C%', MinDiffTimeValue.SensorName)&gt; 0 and PATINDEX('%A%', MinDiffTimeValue.Type)&gt; 0) then 'AC'
			end as 交流電壓類別
			from 
				(

				select Result.* from
				(
					select A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
					,case
					 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 end as DiffTime


					 from 
						MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
						where A.DataID = C.ID
						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區---SUBSTRING(B.SensorName,1,10)as 機櫃名稱,
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51 
						and B.SensorName like '%相'
						and (C.Type like '與%' )
						and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
						--and DATEPART(HH, A.CreateDate) = 15
				)Result
				 join
				(-----------------------------這是作為一個查詢條件

					select  GP_SensorName.SensorName,MIN(DiffTime) MinDiffTime , GP_SensorName.CreatDate 
					from
					(
						select A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
						,case
						 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 end as DiffTime


						 from 
							MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
							where A.DataID = C.ID
							and C.SensorID = B.ID
							and C.ControlID = D.ID
							and D.Factory = @廠區---SUBSTRING(B.SensorName,1,10)as 機櫃名稱,
							and D.Building = @樓棟
							and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
							and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
							and B.SensorName like '%相'
							and (C.Type like '與%' )
							and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)
							--and DATEPART(HH, A.CreateDate) = 15
							
					) GP_SensorName
					group by GP_SensorName.SensorName,GP_SensorName.CreatDate
				)Condition
				on Result.CreatDate = Condition.CreatDate
				and Result.DiffTime = Condition.MinDiffTime
				and Result.SensorName = Condition.SensorName


				) as MinDiffTimeValue
			) 
		as Voltage

	)Voltage1
	group by Voltage1.日期, Voltage1.SensorName
) as Voltage
left join
(

	select top 100 percent GP_Current.日期,GP_Current.SensorName, MAX(GP_Current.A)as A, MAX(GP_Current.B) as B ,MAX(GP_Current.C) as C from
	(

		select top 100 percent Current1.日期, Current1.值, Current1.三相電流類別,Current1.SensorName,
		case when Current1.三相電流類別 = 'A' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'A')
		end as A,
		case when Current1.三相電流類別 = 'B' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'B')
		end as B,
		case when Current1.三相電流類別 = 'C' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'C')
		end as C
		 from
			(
			select top 100 percent   CONVERT(nchar(16),NearValue.CreatDate,120) as 日期, CONVERT(float,Value) as 值, NearValue.Type, SUBSTRING(NearValue.SensorName,1,4) SensorName,
			case 
			when PATINDEX('%C%', NearValue.SensorName) &gt;0  then 'C'
			when PATINDEX('%B%', NearValue.SensorName) &gt;0  then 'B'
			when PATINDEX('%A%', NearValue.SensorName) &gt;0  then 'A'
			end as 三相電流類別
			from 
			(
			
				select top 100 percent Result.CreatDate, Result.Value, Result.SensorName,Result.Type from
				(
					select top 100 percent A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
					,case
					 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
					 end as DiffTime


					 from 
							MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
							where A.DataID = C.ID
							and C.SensorID = B.ID
							and C.ControlID = D.ID
							and D.Factory = @廠區
							and D.Building = @樓棟
							and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
							and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
							and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)

							and B.SensorName like '%相'
							and (C.Type = '電流' )
				)Result
				 join
				(-----------------------------這是作為一個查詢條件

					select top 100 percent  GP_SensorName.SensorName,MIN(DiffTime) MinDiffTime , GP_SensorName.CreatDate 
					from
					(
						select top 100 percent A.ID, CONVERT(char(10),A.CreateDate,120) CreatDate,A.Value,C.Type,B.SensorName
						,case
						 when DATEPART(DD,A.CreateDate) =5 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =15 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 when DATEPART(DD,A.CreateDate) =25 then ABS(DATEDIFF(SECOND,A.CreateDate,DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))))
						 end as DiffTime


						 from 
								MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
								where A.DataID = C.ID
								and C.SensorID = B.ID
								and C.ControlID = D.ID
								and D.Factory = @廠區
								and D.Building = @樓棟
								and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
								and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
								and (DATEPART(DAY, A.CreateDate) = 5 or DATEPART(DAY, A.CreateDate) = 15 or DATEPART(DAY, A.CreateDate) = 25)

								and B.SensorName like '%相'
								and (C.Type = '電流' )
							
					) GP_SensorName
					group by GP_SensorName.SensorName,GP_SensorName.CreatDate
				)Condition
				on Result.CreatDate = Condition.CreatDate
				and Result.DiffTime = Condition.MinDiffTime
				and Result.SensorName = Condition.SensorName
			) as NearValue
		) 
		as Current1
		order by 日期
	)GP_Current
	group by GP_Current.日期, GP_Current.SensorName
) as Current1
on Voltage.日期 = Current1.日期
and Voltage.SensorName = Current1.SensorName
left join

(

	select top 100 percent GP_Current.日期,GP_Current.SensorName, MAX(GP_Current.A)as A, MAX(GP_Current.B) as B ,MAX(GP_Current.C) as C from
	(

		select top 100 percent Current1.日期, Current1.值, Current1.三相電流類別,Current1.SensorName,
		case when Current1.三相電流類別 = 'A' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'A')
		end as A,
		case when Current1.三相電流類別 = 'B' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'B')
		end as B,
		case when Current1.三相電流類別 = 'C' then (select top 100 percent Current1.值 where Current1.三相電流類別 = 'C')
		end as C
		 from
			(
			select top 100 percent   CONVERT(nchar(16),NearValue.CreatDate,120) as 日期, CONVERT(float,Value) as 值, NearValue.Type, SUBSTRING(NearValue.SensorName,1,4) SensorName,
			case 
			when PATINDEX('%C%', NearValue.SensorName) &gt;0  then 'C'
			when PATINDEX('%B%', NearValue.SensorName) &gt;0  then 'B'
			when PATINDEX('%A%', NearValue.SensorName) &gt;0  then 'A'
			end as 三相電流類別
			from 
			(
			

				-----------------------------------------------------------------最大功率
				select CONVERT(char(10),Powered.固定日期,120) as CreatDate, Powered.SensorName, MAX(Powered.值) Value, Powered.Type from
				(
						select  top 100 percent CONVERT(nchar(16),CreateDate,120) as 日期, CONVERT(float,Value) as 值, C.Type, SUBSTRING(B.SensorName,1,10) SensorName
						, CONVERT(nchar(16),DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01')),120) as 固定日期		
						from 
						MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
						where A.DataID = C.ID
						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
						and B.SensorName like '%相'
						and (C.Type = '有功功率' )
						and A.CreateDate &gt;= convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01')
						and A.CreateDate &lt; DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))

				) Powered
				group by Powered.固定日期, Powered.SensorName,Powered.Type
				union
				-----------------------------------------------------------------5到15號最大功率
				select CONVERT(char(10),Powered.固定日期,120) as CreatDate, Powered.SensorName, MAX(Powered.值) Value, Powered.Type from
				(
						select  top 100 percent CONVERT(nchar(16),CreateDate,120) as 日期, CONVERT(float,Value) as 值, C.Type, SUBSTRING(B.SensorName,1,40) SensorName
						, CONVERT(nchar(10),DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01')),120) as 固定日期		
						from 
						MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
						where A.DataID = C.ID
						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
						and B.SensorName like '%相'
						and (C.Type = '有功功率' )
						and A.CreateDate &gt;= DATEADD(MINUTE, 6690,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))
						and A.CreateDate &lt; DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))
				) Powered
				group by Powered.固定日期, Powered.SensorName,Powered.Type
				union

				-----------------------------------------------------------------15到25號最大功率
				select CONVERT(char(10),Powered.固定日期,120) as CreatDate, Powered.SensorName, MAX(Powered.值) Value, Powered.Type from
				(
						select  top 100 percent CONVERT(nchar(16),CreateDate,120) as 日期, CONVERT(float,Value) as 值, C.Type, SUBSTRING(B.SensorName,1,40) SensorName
						, CONVERT(nchar(10),DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01')),120) as 固定日期		
						from 
						MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
						where A.DataID = C.ID
						and C.SensorID = B.ID
						and C.ControlID = D.ID
						and D.Factory = @廠區
						and D.Building = @樓棟
						and D.ComputerRoom = @機房 and isnumeric(A.Value)&gt;0  
						and DATEPART(MM,CreateDate) = @月份 and DATEPART(YEAR,A.CreateDate) = @年份 --添加年份查詢,修改時間:2017-7-17 16:21:51
						and B.SensorName like '%相'
						and (C.Type = '有功功率' )
						and A.CreateDate &gt;= DATEADD(MINUTE, 21090,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))
						and A.CreateDate &lt; DATEADD(MINUTE, 35490,convert(datetime,  CONVERT(nchar(4),DATEPART(YEAR,GETDATE()))  +'-'+@月份+'-' +'01'))
				) Powered
				group by Powered.固定日期, Powered.SensorName,Powered.Type
			) as NearValue
		) 
		as Current1
		order by 日期
	)GP_Current
	group by GP_Current.日期, GP_Current.SensorName
) as PMax
on Voltage.日期 = PMax.日期
and Voltage.SensorName = PMax.SensorName
left join 
(

	select top 100 percent GP_Current.日期,GP_Current.SensorName, MAX(GP_Current.A)as A, MAX(GP_Current.B) as B ,MAX(GP_Current.C) as C from
	(

		select top 100 percent Current1.日期, Current1.值, Current1.三相電流類別,Current1.SensorName,
