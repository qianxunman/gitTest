      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@廠區">
            <Value>=Parameters!廠區.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@樓棟">
            <Value>=Parameters!樓棟.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@機房">
            <Value>=Parameters!機房.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@月份">
            <Value>=Parameters!月份.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@年份">
            <Value>=Parameters!年份.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
--=========================================
--開發人:羅俊,時間:2017-7-19 09:34:48
--新增頁面——異常事件統計（by硬件）
--說明：
--1.統計每個設備每日生成的事件數
--2.若月份為當前月：數據統計範圍為自本月1號至查詢日；若為往月：統計範圍為該月1號至月末
--=====================================================================================

--Set The time,include StartTime and EndTime
declare @StartTime datetime,@EndTime datetime
if(DATEPART(MONTH,GETDATE()) = @月份 and (DATEPART(YYYY,GETDATE()) = @年份))
begin
set @StartTime = CONVERT(char(4),@年份) +'-'+CONVERT(nvarchar(2),@月份) +'-01'
set @EndTime = GETDATE()
end
else
begin
set @StartTime = CONVERT(char(4),@年份) +'-'+CONVERT(nvarchar(2),@月份) +'-01'
set @EndTime = DATEADD(MONTH,1,@StartTime)
end


select T1.SensorName,T1.CreateDate,COUNT(*) as _count from 
(
	select
	case when B.SensorName like '%機櫃%' then CONVERT(char(6),B.SensorName)
	when B.SensorName like 'UPS%' then CONVERT(char(12),B.SensorName)
	else B.SensorName
	end as SensorName
	,CONVERT(char(10),A.CreateDate,120) as CreateDate

	from   ExceptionDeal as A,SensorInfo as B,AddressInfo as C, ControlInfo as D
	where  A.DataID = C.ID
	and C.ControlID = D.ID 
	and C.SensorID = B.ID
	and D.Factory = @廠區---B.SensorName like '%電力'
	and D.Building = @樓棟
	and D.ComputerRoom = @機房
	and A.CreateDate&gt;@StartTime
	and A.CreateDate&lt;@EndTime


)as T1
group by T1.CreateDate,T1.SensorName


--select  @StartTime,@EndTime</CommandText>
      </Query>
      <Fields>
        <Field Name="CreateDate">
          <DataField>CreateDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="SensorName">
          <DataField>SensorName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ID_count">
          <DataField>_count</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
