    <DataSet Name="電箱線纜溫度分析">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@廠區">
            <Value>=Parameters!廠區.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@樓棟">
            <Value>=Parameters!樓棟.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@機房">
            <Value>=Parameters!機房.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@月份">
            <Value>=Parameters!月份.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@年份">
            <Value>=Parameters!年份.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@設備">
            <Value>=Parameters!設備.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>
declare @StartYear datetime,@EndYear datetime
set @StartYear = CONVERT (char(4),@年份)+'-01-01'
set @EndYear = DATEADD(YEAR,1,CONVERT (char(4),@年份)+'-01-01')

select * from 
(

select Group機櫃名稱.日期 as 日期, avg(Group機櫃名稱.值) as 值,MAX(Group機櫃名稱.值) as MaxValue, SUBSTRING(Group機櫃名稱.機櫃名稱,1,8) as 電箱名稱, SUBSTRING(Group機櫃名稱.機櫃名稱, 9,2) 位置 
,case when SUBSTRING(Group機櫃名稱.機櫃名稱, 9,2) = '箱內' then SUBSTRING(Group機櫃名稱.機櫃名稱,1,6) +'電箱'
else SUBSTRING(Group機櫃名稱.機櫃名稱,1,6) +SUBSTRING(Group機櫃名稱.機櫃名稱, 9,2)
end as SensorName
from
(
select CONVERT(nchar(10),CreateDate,120) as 日期, CONVERT(float,Value) as 值, SUBSTRING(B.SensorName,1,10)as 機櫃名稱
from MonitData as A, SensorInfo as B, AddressInfo as C, ControlInfo as D
where A.DataID = C.ID
and C.SensorID = B.ID
and C.ControlID = D.ID
and D.Factory = @廠區---B.SensorName like '%電力'
and D.Building = @樓棟
and D.ComputerRoom = @機房
--and A.CreateDate &gt; CONVERT (nvarchar(4),@年份)+'-01-01' --修改時間:2017-7-17 16:21:51
and CreateDate&gt;@StartYear
and CreateDate&lt;@EndYear--2017-8-7 15:06:45
and DATEPART(MM,CreateDate) = @月份
and C.Type = '溫度'
and B.SensorName like 'UPS%'
and ISNUMERIC(A.Value)&gt;0
) as Group機櫃名稱
group by Group機櫃名稱.日期,Group機櫃名稱.機櫃名稱


) as TB
where TB.SensorName like +'%'+@設備+'%' or @設備 = ' '</CommandText>
      </Query>
      <Fields>
        <Field Name="日期">
          <DataField>日期</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="平均溫度">
          <DataField>值</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="電箱名稱">
